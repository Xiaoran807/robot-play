<?xml version="1.0" encoding="UTF-8"?>

<launch>
    
    <arg name="x" default="-5" />
    <arg name="y" default="0" />
    <arg name="z" default="-8" />
    <arg name="roll" default="0"/>
    <arg name="pitch" default="0"/>
    <arg name="yaw" default="0" />
    
    <arg name="urdf_robot_file" default="$(find gazebo_description)/urdf/objectUnderwater.urdf" />
    <arg name="robot_name" default="objectUnderwater" />
    
    <include file="$(find gazebo_description)/launch/spawn_box_urdf.launch">
        <arg name="x" value="$(arg x)" />
        <arg name="y" value="$(arg y)" />
        <arg name="z" value="$(arg z)" />
        <arg name="roll" value="$(arg roll)"/>
        <arg name="pitch" value="$(arg pitch)"/>
        <arg name="yaw" value="$(arg yaw)" />
        
        <arg name="urdf_robot_file" value="$(arg urdf_robot_file)" />
        <arg name="robot_name" value="$(arg robot_name)" />
    </include>
</launch>
