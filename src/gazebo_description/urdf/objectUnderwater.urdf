<robot name="objectUnderwater">
 
 <!-- Colours for RVIZ for geometric elements -->
    <material name="blue">
        <color rgba="0 0 0.8 1"/>
    </material>
    <material name="red">
        <color rgba="0.8 0 0 1"/>
    </material>
    <material name="green">
        <color rgba="0 0.8 0 1"/>
    </material>
    <material name="grey">
        <color rgba="0.75 0.75 0.75 1"/>
    </material>
    <material name="white">
        <color rgba="1.0 1.0 1.0 1"/>
    </material>
    <material name="black">
        <color rgba="0 0 0 1"/>
    </material>


 <link name="my_box">
    <inertial>
      <origin xyz="-5 0 -8" />
      <mass value="0.00001" />
      <inertia  ixx="1.0" ixy="0.0"  ixz="0.0"  iyy="100.0"  iyz="0.0"  izz="1.0" />
    </inertial>
    <visual>
      <origin xyz="2 0 1"/>
      <geometry>
        <box size="5 5 8" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="2 0 1"/>
      <geometry>
        <box size="5 5 8" />
      </geometry>
    </collision>
  </link>

 <link name="front_smallbox">
    <visual>
      <geometry>
        <box size="0.5 0.5 0.5"/>
      </geometry>
      <origin rpy="0 0 0" xyz="4.7 0 .2"/>
      <material name="green"/>
    </visual>
    <collision>
      <origin xyz="1.4 0 .2"/>
      <geometry>
        <box size=".2 .2 .2" />
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0" />
      <mass value="1"/>
      <inertia ixx="0.4" ixy="0.0" ixz="0.0" iyy="0.4" iyz="0.0" izz="0.2"/>
    </inertial>
  </link>
  <joint name="base_to_right_leg" type="fixed">
    <parent link="my_box"/>
    <child link="front_smallbox"/>
    <origin xyz="0 0 1"/>
  </joint>


  <gazebo reference="my_box">
    <material>Gazebo/white</material>
    <turnGravityOff>true</turnGravityOff>
  </gazebo>
<gazebo reference="front_smallbox">
    <turnGravityOff>true</turnGravityOff>
  </gazebo>

  <gazebo>
        <plugin name="p3d_base_controller" filename="libgazebo_ros_p3d.so">
      <alwaysOn>true</alwaysOn>
      <updateRate>50.0</updateRate>
      <bodyName>front_smallbox</bodyName>
      <topicName>ground_truth/stateRexRov</topicName>
      <gaussianNoise>0.01</gaussianNoise>
      <frameName>world</frameName>
      <xyzOffsets>0 0 0</xyzOffsets>
      <rpyOffsets>0 0 0</rpyOffsets>
    </plugin>
  <gazebo>
</robot>
